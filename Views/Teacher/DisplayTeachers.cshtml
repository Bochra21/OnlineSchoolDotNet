@model List<OnlineSchoolWebApp.Models.ApplicationUser>

    <h4>Teachers List</h4>
<table class="table table-hover">
    <thead>
        <tr>

            <th>First Name</th>
            <th>Last Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr class="table-active">


                <td>@user.FirstName</td>
                <td>@user.LastName</td>
            </tr>
        }
    </tbody>
</table>
